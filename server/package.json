    "babel-core": "^6.18.0",
    "bcrypt": "^1.0.0",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "helmet": "^3.1.0",
    "jsonwebtoken": "^7.1.0",
    "lodash": "^4.16.4",
    "method-override": "^2.3.6",
    "mongoose": "^4.6.5",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "bluebird": "^3.4.6",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express-jwt": "^5.1.0",
    "express-validation": "^1.0.1",
